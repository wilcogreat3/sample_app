ApplicationHelper	.\application_helper.rb	/^module ApplicationHelper$/;"	m
SessionsHelper	.\sessions_helper.rb	/^module SessionsHelper$/;"	m
StaticPagesHelper	.\static_pages_helper.rb	/^module StaticPagesHelper$/;"	m
UsersHelper	.\users_helper.rb	/^module UsersHelper$/;"	m
current_user	.\sessions_helper.rb	/^  def current_user$/;"	f	class:SessionsHelper
current_user=	.\sessions_helper.rb	/^  def current_user=(user)$/;"	f	class:SessionsHelper
full_title	.\application_helper.rb	/^  def full_title(page_title)$/;"	f	class:ApplicationHelper
gravatar_for	.\users_helper.rb	/^  def gravatar_for(user)$/;"	f	class:UsersHelper
sign_in	.\sessions_helper.rb	/^  def sign_in(user)$/;"	f	class:SessionsHelper
sign_out	.\sessions_helper.rb	/^  def sign_out$/;"	f	class:SessionsHelper
signed_in?	.\sessions_helper.rb	/^  def signed_in?$/;"	f	class:SessionsHelper
